{
    "name": "gnahotelsolutions/flasher",
    "description": "Flash notifications in Laravel",
    "authors": [
        {
            "name": "Maria Carrero",
            "email": "mcarrero@gna.es"
        },
        {
            "name": "David Llop",
            "email": "dllop@gna.es"
        }
    ],
    "type": "library",
    "require": {
        "php": "^7.1|^8.0|^8.1",
        "laravel/framework": "^5.7|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2|^8.0|^9.0",
        "orchestra/testbench": "^3.7|^4.0|^5.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "GNAHotelSolutions\\Flasher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GNAHotelSolutions\\Flasher\\FlasherServiceProvider"
            ]
        }
    },
    "license": "MIT"
}
